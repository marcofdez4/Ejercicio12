    agent any
    stages {
        stage('Build') {
            steps {
                script {
                    echo 'Building Mi proyecto (Build #1)'
                }
            }
        }
        stage('Barcelona') {
            steps {
                script {
                    echo 'Poblacion final de Barcelona es: 80000.0'
                }
            }
        }
        stage('Huelva') {
            steps {
                script {
                    echo 'Poblacion final de Huelva es: 160000.0'
                }
            }
        }
        stage('Madrid') {
            steps {
                script {
                    echo 'Poblacion final de Madrid es: 800000.0'
                }
            }
        }
        stage('Malaga') {
            steps {
                script {
                    echo 'Poblacion final de Malaga es: 40000.0'
                }
            }
        }
        stage('Sevilla') {
            steps {
                script {
                    echo 'Poblacion final de Sevilla es: 64000.0'
                }
            }
        }
        stage('Deploy') {
            steps {
                script {
                    echo 'Deployment activo...'
                }
            }
        }
    }
}